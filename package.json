    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lorem-ipsum": "^2.0.8",
    "mongoose": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
